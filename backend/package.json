{
    "name": "cs-backend",
    "version": "1.0.0",
    "description": "URL shortener service with PostgreSQL and Node.js",
    "main": "src/app.js",
    "scripts": {
        "start": "node --expose-gc --max-old-space-size=8192 src/app.js",
        "dev": "nodemon --expose-gc src/app.js",
        "start:prod": "NODE_ENV=production node --expose-gc --max-old-space-size=2048 src/app.js",
        "queue": "node src/workers/urlProcessor.js",
        "test": "jest",
        "lint": "eslint src/**/*.js",
        "migrate": "sequelize-cli db:migrate",
        "migrate:undo": "sequelize-cli db:migrate:undo",
        "seed": "sequelize-cli db:seed:all"
    },
    "keywords": [
        "url-shortener",
        "nodejs",
        "postgresql",
        "express",
        "sequelize"
    ],
    "author": "Your Name",
    "license": "ISC",
    "dependencies": {
        "bull": "^4.16.4",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^6.1.5",
        "ioredis": "^5.3.2",
        "pg": "^8.10.0",
        "pg-hstore": "^2.3.4",
        "rate-limit-redis": "^4.1.0",
        "sequelize": "^6.31.0",
        "valid-url": "^1.0.9"
    },
    "devDependencies": {
        "eslint": "^8.38.0",
        "jest": "^29.5.0",
        "nodemon": "^3.1.7",
        "sequelize-cli": "^6.6.0"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}